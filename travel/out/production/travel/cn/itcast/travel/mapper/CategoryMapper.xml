<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.itcast.travel.mapper.CategoryMapper">

    <select id="findAllCategory" resultType="category">
        select * from tab_category
    </select>

    <select id="findCategoryByCid" parameterType="int" resultType="category">
        select * from tab_category where cid = #{cid}
    </select>

</mapper>




